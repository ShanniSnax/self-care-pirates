abel>\n        <Input type=\"email\" name=\"email\" id=\"mail\" placeholder=\"Email\" onChange = { e => setEmail (e.target.value)} value = {email}/>\n      </FormGroup>\n      <FormGroup>\n        <Label for=\"password\">Password </Label>\n        <Input type=\"password\" name=\"password\" id=\"password\" placeholder=\"Password 6+ characters\" onChange = { e => setPassword (e.target.value)} value = {password}/>\n      </FormGroup>\n      <Row form>\n        <Col>\n          <FormGroup>\n            <Label for=\"userName\">Username</Label>\n            <Input type=\"text\" name=\"userName\" id=\"User Name\" placeholder=\"Username 4+ characters\" onChange = { e => setUserName (e.target.value)} value = {userName}/>\n          </FormGroup>\n        </Col>\n      </Row>\n      {/* //sign up page redirects to forum page  */}\n      <Button onClick = {handleSubmit}>Sign up</Button> {}  \n    {/* //Sign in button that redirects the  user to the login page if they hve already signed up*/}\n      <Button onClick = {() => history.push(\"/login\")}>Log in</Button>\n    </Form>\n  );\n}\n\nexport default Signup;","/Users/tammy/Documents/Bootcamp/self-care-pirates/client/src/components/ProfileCard/Avatar.js",[],"/Users/tammy/Documents/Bootcamp/self-care-pirates/client/src/components/Tags/Tags.js",[],"/Users/tammy/Documents/Bootcamp/self-care-pirates/client/src/components/PhotoModal/PhotoModal.js",[],{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":5,"column":49,"nodeType":"134","messageId":"135","endLine":5,"endColumn":56},{"ruleId":"132","severity":1,"message":"136","line":7,"column":22,"nodeType":"134","messageId":"135","endLine":7,"endColumn":30},{"ruleId":"132","severity":1,"message":"137","line":1,"column":27,"nodeType":"134","messageId":"135","endLine":1,"endColumn":36},{"ruleId":"132","severity":1,"message":"138","line":2,"column":10,"nodeType":"134","messageId":"135","endLine":2,"endColumn":13},{"r